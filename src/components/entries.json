{
  "Personal Information": {
    "className": "personalSection",
    "code": "ps",
    "extendable": false,
    "fields": [
      { "name": "firstName", "label": "First Name", "classes": "singleCol" },
      { "name": "lastName", "label": "Last Name", "classes": "singleCol" },
      { "name": "emailAdd", "label": "Email Address", "inputType": "email" },
      {
        "name": "phoneNum",
        "label": "Phone Number",
        "inputType": "tel",
        "pattern": "^(1\\s?)?(\\d{3}|\\(\\d{3}\\))[\\s\\-]?\\d{3}[\\s\\-]?\\d{4}$"
      }
    ]
  },
  "Educational Background": {
    "className": "educationSection",
    "code": "ed",
    "extendable": true,
    "fields": [
      { "name": "yearRange", "label": "Year(s)", "classes": "singleCol" },
      { "name": "school", "label": "School" }
    ]
  }
}
